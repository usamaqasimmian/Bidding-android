        <activity android:name=".TutorOfferForm" />
        <activity android:name=".MessagePrivate" />
        <activity android:name=".PostOfferMessage" />
        <activity android:name=".ViewAllOffers" />
        <activity android:name=".StudViewOffers" />


        <receiver android:name=".TimeOutBid"
            android:permission="TODO"
            tools:ignore="ExportedReceiver">
            <intent-filter>
                <action android:name="processTimerReceiver" >
                </action>
            </intent-filter>
        </receiver>
    </application>

</manifest>